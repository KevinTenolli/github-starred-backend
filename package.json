    "build": "tsc",
    "lint": "eslint . --ext .ts",
    "lint:fix": "eslint . --ext .ts --fix",
    "typeorm": "typeorm",
    "migration:generate": "TS_NODE=true ts-node -r tsconfig-paths/register ./node_modules/typeorm/cli.js migration:generate -d ./src/ormconfig.migration.ts",
    "migration:create": "TS_NODE=true ts-node -r tsconfig-paths/register ./node_modules/typeorm/cli.js migration:create",
    "migration:revert": "TS_NODE=true ts-node -r tsconfig-paths/register ./node_modules/typeorm/cli.js migration:revert -d ./src/ormconfig.migration.ts",
    "migration:run": "TS_NODE=true ts-node -r tsconfig-paths/register ./node_modules/typeorm/cli.js migration:run -d ./src/ormconfig.migration.ts",
    "migration:generate:docker": "TS_NODE=true ts-node -r tsconfig-paths/register ./node_modules/typeorm/cli.js migration:generate -d ./src/migration.ts",
    "migration:revert:docker": "TS_NODE=true ts-node -r tsconfig-paths/register ./node_modules/typeorm/cli.js migration:revert -d ./src/migration.ts",
    "migration:run:docker": "TS_NODE=true ts-node -r tsconfig-paths/register ./node_modules/typeorm/cli.js migration:run -d ./src/migration.ts"
  },
